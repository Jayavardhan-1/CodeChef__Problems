import java.util.*;
import java.lang.*;
import java.io.*;

class Codechef
{
	public static void main (String[] args) throws java.lang.Exception
	{
		// your code goes here
        Scanner sc=new Scanner(System.in);
        int n=sc.nextInt();
        int res=5*n;
        if(res>60)  
            System.out.println("NO");
        else
           System.out.println("YES");
	}
}
