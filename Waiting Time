import java.util.*;
import java.lang.*;
import java.io.*;

class Codechef
{
	public static void main (String[] args) throws java.lang.Exception
	{
		// your code goes here
        
        Scanner sc=new Scanner(System.in);
        int h=sc.nextInt();
        while(h-->0){
            int a=sc.nextInt();
            int b=sc.nextInt();
            int c=a*7;
            int d=c-b;
            System.out.println(d);
        }
	}
}
