import java.util.*;
import java.lang.*;
import java.io.*;

class Codechef
{
	public static void main (String[] args) throws java.lang.Exception
	{
		// your code goes here
        Scanner sc=new Scanner(System.in);
        int h=sc.nextInt();
        if(h<3)
            System.out.println("GOLD");
        else if(h>=3 && h<6)
            System.out.println("SILVER");
        else
            System.out.println("BRONZE");
	}
}
