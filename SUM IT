import java.util.*;
import java.lang.*;
import java.io.*;

class Codechef
{
	public static void main (String[] args) throws java.lang.Exception
	{
		// your code goes here
        Scanner sc=new Scanner(System.in);
        int h=sc.nextInt();
        while(h-->0){
            int u=sc.nextInt();
            int v=sc.nextInt();
            int w=sc.nextInt();
            int r1=u+v;
            if(r1==w)
               System.out.println("YES");
            else
               System.out.println("NO");
        }
	}
}
