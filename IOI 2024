import java.util.*;
import java.lang.*;
import java.io.*;

class Codechef
{
	public static void main (String[] args) throws java.lang.Exception
	{
		// your code goes here
        Scanner sc=new Scanner(System.in);
        int h=sc.nextInt();
        if(h>=1 && h<=8)
           System.out.println("YES");
        else
           System.out.println("NO");
	}
}
