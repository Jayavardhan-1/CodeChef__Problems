import java.util.*;
import java.lang.*;
import java.io.*;

class Codechef
{
	public static void main (String[] args) throws java.lang.Exception
	{
		// your code goes here
         Scanner sc=new Scanner(System.in);
         int r=sc.nextInt();
         int c=sc.nextInt();
         int ans=r-c;
         if(ans>=18)
             System.out.println("RCB");
         else
            System.out.println("CSK");
	}
}
