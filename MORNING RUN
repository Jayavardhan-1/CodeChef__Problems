import java.util.*;
import java.lang.*;
import java.io.*;

class Codechef
{
	public static void main (String[] args) throws java.lang.Exception
	{
		// your code goes here
        Scanner sc=new Scanner(System.in);
        int u=sc.nextInt();
        int v=sc.nextInt();
        int ans=u+u+v+v;
        if(ans<1000)
           System.out.println("NO");
        else
          System.out.println("YES");
	}
}
