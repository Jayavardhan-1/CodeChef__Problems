import java.util.*;
import java.lang.*;
import java.io.*;

class Codechef
{
	public static void main (String[] args) throws java.lang.Exception
	{
		// your code goes here
        Scanner sc=new Scanner(System.in);
        int u=sc.nextInt();
        int v=sc.nextInt();
        int w=sc.nextInt();
        int ans=15-(u+v+w);
        System.out.println(ans);
	}
}
